<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<article xmlns="http://sweble.org/schema/wom30"
         xmlns:mww="http://sweble.org/schema/mww30"
         version="3.0"
         title="Img-link">
   <body>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle">
            <rtd>[[Image:Test]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                alt="Alt Text">
            <rtd>[[Image:Test|alt=Alt Text]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                alt="Alt Text">
            <rtd>[[Image:Test|alt=Alt Text</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                intlink="">
            <rtd>[[Image:Test|link=</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                intlink=" ">
            <rtd>[[Image:Test|link= </rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                extlink="http://example.com">
            <rtd>[[Image:Test|link=http://example.com</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                intlink="Some Page">
            <rtd>[[Image:Test|link=Some Page</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                border="border"
                halign="default"
                valign="middle"
                upright="upright"
                alt="Alt Text">
            <rtd>[[Image:Test|alt=Alt Text|upright|border</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <image source="Image:Test"
             format="frame"
             border="border"
             halign="right"
             valign="bottom"
             upright="upright"
             alt="Alt Text">
         <rtd>[[Image:Test|alt=Alt Text|frame|bottom|right|upright|border</rtd>
         <imgcaption>
            <rtd>|</rtd>
            <text>Title</text>
         </imgcaption>
         <rtd>]]</rtd>
      </image>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                width="128"
                alt="Alt Text">
            <rtd>[[Image:Test|alt=Alt Text|128px</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                height="512"
                alt="Alt Text">
            <rtd>[[Image:Test|alt=Alt Text|x512px</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>

</text>
      <p topgap="0" bottomgap="0">
         <image source="Image:Test"
                format="unrestrained"
                halign="default"
                valign="middle"
                width="128"
                height="512"
                alt="Alt Text">
            <rtd>[[Image:Test|alt=Alt Text|128x512px</rtd>
            <imgcaption>
               <rtd>|</rtd>
               <text>Title</text>
            </imgcaption>
            <rtd>]]</rtd>
         </image>
      </p>
      <text>
</text>
   </body>
</article>
