<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<article xmlns="http://sweble.org/schema/wom30"
         xmlns:mww="http://sweble.org/schema/mww30"
         version="3.0"
         title="Xml-char-ref">
   <body>
      <p topgap="0" bottomgap="0">
         <subst>
            <repl>
               <text>&amp;#20;</text>
            </repl>
            <for>
               <mww:xml-char-ref code="20">
                  <rtd>&amp;#20;</rtd>
               </mww:xml-char-ref>
            </for>
         </subst>
         <text>
</text>
         <subst>
            <repl>
               <text> </text>
            </repl>
            <for>
               <mww:xml-char-ref code="32">
                  <rtd>&amp;#32;</rtd>
               </mww:xml-char-ref>
            </for>
         </subst>
         <text>
</text>
         <subst>
            <repl>
               <text>A</text>
            </repl>
            <for>
               <mww:xml-char-ref code="65">
                  <rtd>&amp;#x41;</rtd>
               </mww:xml-char-ref>
            </for>
         </subst>
      </p>
   </body>
</article>
